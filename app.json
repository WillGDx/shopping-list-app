{
  "expo": {
    "name": "Minha Lista de Compras",   
    "slug": "minha-lista-de-compras",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,            
    "splash": {
      "image": "./assets/splash.png", 
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [           
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "willcode.minhalistadecompras"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "willcode.minhalistadecompras",
      "edgeToEdgeEnabled": true      
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}